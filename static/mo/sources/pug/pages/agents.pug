extends  ../_skeleton

//- Includes
include ../components/_ui-block-agent-horizontal
	
block variables
	- var activeCategory = 'pages'
	- var pageName = 'Agents'

block page
	include ../sections/_header-classic

	+breadcrumbs({
		category: activeCategory,
		path: [pageName],
		bgImage: 'breadcrumbs-bg-04-1922x367',
		data: '37'
	})

	// Section Agent
	section.section.section-md.bg-gray-12
		.container
			.row.row-30
				.col-lg-7.col-xl-8
					.pdr-xl-35
						.row.row-30.row-lg-65
							.col-sm-6.col-md-12
								+block-agent-horizontal({
									img: 'agents-01-540x460',
									title: 'Michael Rutter',
									text: 'Certified Real Estate Broker'
								})
							.col-sm-6.col-md-12
								+block-agent-horizontal({
									img: 'agents-02-540x460',
									title: 'Janet Richmond',
									text: 'Residential Real Estate Broker'
								})
							.col-sm-6.col-md-12
								+block-agent-horizontal({
									img: 'agents-03-540x460',
									title: 'Sam Wilson',
									text: 'Real Estate Broker'
								})
							.col-sm-6.col-md-12
								+block-agent-horizontal({
									img: 'agents-04-540x460',
									title: 'Mary Peterson',
									text: 'Real Estate Broker'
								})
								
				.col-lg-5.col-xl-4
					include ../includes/_agent-sidebar-right


	include ../sections/_footer-classic
