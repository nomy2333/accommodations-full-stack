extends  ../_skeleton

//- Includes
include ../components/_ui-box-minimal
include ../components/_ui-box-modern

block variables
	- var activeCategory = 'pages'
	- var pageName = 'Typography'

block page 
	// Page Header
	include ../sections/_header-classic

	+breadcrumbs({
		category: activeCategory,
		path: [pageName],
		bgImage: 'breadcrumbs-bg-01-1922x291'
	})

	// Base typography 
	section.section.section-lg.bg-default
		.container
			.row.row-50.flex-md-row-reverse
				.col-md-4
					.ml-md-minus-20
						h1 H1 Heading
						.mt-10
							h2.text-spacing-40 H2 Heading
						.mt-15
							h3 H3 Heading
						.mt-20
							h4 H4 Heading
						.mt-20
							h5.text-spacing-0 H5 Heading
						.mt-15
							h6.text-spacing-0 H6 Heading

				.col-md-8
					.pdr-md-30
						h1 H1 Heading
						p.text-gray-1.mt-30 Welcome to our wonderful world. We sincerely hope that each and every user entering our website will find exactly what he/she is looking for. With advanced features of activating account and new login widgets, you will definitely have a great experience of using our web page.
